                        <Border HorizontalAlignment="Left" x:Name="PART_Indicator" BorderBrush="{DynamicResource NormalBorderBrush}"
                                Background="{DynamicResource GlyphBrush}" BorderThickness="1" CornerRadius="3" />
                        <Grid x:Name="IndeterminateRoot" Visibility="Collapsed">
