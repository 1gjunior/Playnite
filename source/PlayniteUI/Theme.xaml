    <SolidColorBrush x:Key="NormalBrush" Color="{StaticResource MainColor}" />
    <SolidColorBrush x:Key="NormalBrushDark" Color="{StaticResource MainColorDark}" />
    <SolidColorBrush x:Key="NormalBorderBrush" Color="{StaticResource MainColor}" />
    <SolidColorBrush x:Key="HoverBrush" Color="{StaticResource IndicatorColor}" />
