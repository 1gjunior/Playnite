            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter Property="BorderBrush" Value="{StaticResource GlyphBrush}" />
            </Trigger>
        </Style.Triggers>
    </Style>
</ResourceDictionary>




