    <SolidColorBrush x:Key="LightBrush" Color="#FF333333" />
    <SolidColorBrush x:Key="ShadeBrush" Color="#FF434343" />
    <SolidColorBrush x:Key="ControlBackgroundBrush" Color="{StaticResource MainColorDark}" />

